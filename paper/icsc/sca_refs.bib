@MASTERSTHESIS{SWUI11,
  author = {Ghislain Hachey},
  title = {Semantic Web User Interface: A Systematic Survey},
  school = {Athabasca University},
  year = {2011},
  abstract = {Research on novel user interfaces is essential for the success of
	the Semantic Web vision. Reseachers have
	
	already been proposing various solutions to the problems of presenting
	and interacting with Semantic Web
	
	data for various users groups. It is necessary to systematically analyse
	the research on the subject in
	
	order to evaluate its state and maturity. A challenge we address in
	this paper by reporting the results of
	
	a systematic survey of the literature on Semantic Web user interfaces
	published between 2003 and 2010
	
	following the methods for systematic reviews already adopted in computer
	science research. The survey
	
	has three main aims: to describe and categorize research contributions
	by following the content analysis
	
	method; to approximate the level of quality of research evaluation
	which is later refered to as the \level of
	
	thoroughness"; and, identify gaps and bring about discussion on the
	previous two aims. A set of categories
	
	of research topics related to the Semantic Web user interfaces is
	defined. The set of categories is used to
	
	identify analyse papers and the subject of study as a whole and identify
	gaps using descriptive statistics and
	
	temporal trends. The papers level of thoroughness is reported and
	categorised into three groups: \Low",
	
	\Medium" and \High". Papers in the medium group are further analysed
	for research thoroughness based
	
	on an 11 criteria research instrument adopted from the medical research
	field. The ndings are discussed,
	
	research gaps are identied and potential opportunities are raised
	to motivate future research on Semantic
	
	Web user interfaces.},
  file = {:C\:\\Users\\ali1k\\Desktop\\pub\\SLR primary studies\\SW UIs.pdf:PDF},
  owner = {ali1k},
