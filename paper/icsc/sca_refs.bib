@INCOLLECTION{Klebeck2011,
  author = {Klebeck, Alex and Hellmann, Sebastian and Ehrlich, Christian and
	Auer, Sören},
  title = {OntosFeeder – A Versatile Semantic Context Provider for Web Content
	Authoring},
  booktitle = {The Semanic Web: Research and Applications},
